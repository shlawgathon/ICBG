{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2021"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["./**/*.ts"],
  "exclude": ["_generated"]
}
